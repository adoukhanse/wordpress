    "shopengine_alignment": {
        "control": {
            "field": "SelectControl",
            "label": "Alignment",
            "props": {
                "options": [{
                        "label": "Left",
                        "value": "left"
                    },
                    {
                        "label": "Center",
                        "value": "center"
                    },
                    {
                        "label": "Right",
                        "value": "right"
                    }
                ]
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": "left",
            "tablet": "left",
            "mobile": "left"
        }
    },
