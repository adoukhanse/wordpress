                    },
                    {
                        "label": "300",
                        "value": "300"
                    },
                    {
                        "label": "400",
                        "value": "400"
                    },
                    {
                        "label": "500",
                        "value": "500"
                    },
                    {
                        "label": "600",
                        "value": "600"
                    },
                    {
                        "label": "700",
                        "value": "700"
                    },
                    {
                        "label": "800",
                        "value": "800"
                    },
                    {
                        "label": "900",
                        "value": "900"
                    },
                    {
                        "label": "Default",
                        "value": "normal"
                    },
                    {
                        "label": "Normal",
                        "value": "normal"
                    },
                    {
                        "label": "Bold",
                        "value": "bold"
                    }
                ]
            }
        },
        "responsive": false,
        "type": "object",
        "default": {
            "desktop": "400",
            "tablet": "400",
            "mobile": "400"
        }
    },
    "shopengine_result_count_font_style": {
        "control": {
            "field": "SelectControl",
            "label": "Style",
            "props": {
                "options": [{
                        "label": "Default",
