{
    "general_start": {
        "type": "PanelBody",
        "label": "General"
    },
    "products_per_page": {
        "control": {
            "field": "__experimentalNumberControl",
            "label": "Products Per Page",
            "props": {
                "max": 50,
                "min": 1
            }
        },
        "type": "object",
        "default": {
            "desktop": 6
        }
    },
    "product_order": {
        "control": {
            "field": "SelectControl",
            "label": "Order",
            "props": {
                "options": [
                    {
                        "label": "DESC",
                        "value": "DESC"
                    },
                    {
                        "label": "ASC",
                        "value": "ASC"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": "DESC"
        }
    },
    "column": {
        "control": {
            "field": "__experimentalNumberControl",
            "label": "Column",
            "props": {
                "max": 12,
                "min": 1,
                "step": 1
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": 6
        }
    },
    "general_end": {
        "type": "PanelBody",
        "label": "General"
    },
