{
    "shopengine_heading_panel_start":{
        "type":"PanelBody",
        "label":"Title"
    },
     "shopengine_title_heading_input":{
        "control":{
           "field": "TextareaControl",
           "props":{
            "label": "Title",
             "options": [
               {
                "label": "Shopengine Title",
                "value": "Hello World"
               }
             ]
           }
        },
        "responsive": false,
         "type": "object",
         "default": {
            "desktop": "Shopengine Title"
         }
     },

      "shopengine_title_header_size": {
         "control": {
            "field": "SelectControl",
            "label": "HTML Tag",
            "props": {
               "options": [
                     {
                        "label": "H1",
                        "value": "h1"
                     },
                     {
                        "label": "H2",
                        "value": "h2"
                     },
                     {
                        "label": "H3",
                        "value": "h3"
                     },
                     {
                        "label": "H4",
                        "value": "h4"
                     },
                     {
                        "label": "H5",
                        "value": "h5"
                     },
                     {
                        "label": "H6",
                        "value": "h6"
                     },
                     {
                        "label": "Div",
                        "value": "div"
                     },
                     {
                        "label": "Span",
                        "value": "span"
                     },
                     {
                        "label": "P",
                        "value": "p"
                     }
               ]
            }
         },
         "responsive": false,
         "type": "object",
         "default": {
            "desktop": "h4"
         }
   },
   "shopengine_heading_title_align": {
         "control": {
            "field": "SelectControl",
            "label": "Alignment",
            "props": {
               "options": [{
                        "label": "Left",
                        "value": "left"
                     },
                     {
                        "label": "Center",
                        "value": "center"
                     },
                     {
                        "label": "Right",
                        "value": "right"
                     }
               ]
            }
         },
         "responsive": true,
         "type": "object",
         "default": {
            "desktop": "left",
            "tablet": "left",
            "mobile": "left"
         }
   },
  "shopengine_heading_panel_end":{
      "type":"PanelBody"
  },

   "shopengine_heading_style_panel_start":{
      "type":"PanelBody",
      "label":"Title Style"
   },

   "shopengine_heading_title_color": {
         "control": {
            "field": "ColorPalette",
            "label": "Text Color",
            "props": {
               "colors": [{
                        "name": "red",
                        "color": "#f00"
                     },
                     {
                        "name": "Black",
                        "color": "#101010"
                     },
                     {
                        "name": "blue",
                        "color": "#00f"
                     }
               ]
            }
         },
         "responsive": false,
         "type": "object",
         "default": {
            "desktop": "#101010"
         }
   },
   "shopengine_heading_title_typography_start": {
      "type": "PopoverControl",
      "label": "Text Typography"
  },
  "shopengine_heading_title_font_family": {
      "control": {
          "field": "FontfamilyControl",
          "label": "Font Family"
      },
      "type": "object"
  },
  "shopengine_heading_title_font_size": {
      "control": {
          "field": "RangeControl",
          "label": "Font Size(px)",
          "props": {
             "withInputField": true,
              "allowReset": true,
              "min": 0,
              "max": 200,
              "step": 1
          }
      },
      "responsive": true,
      "type": "object",
      "default": {
          "desktop": ""
      }
  },
  "shopengine_heading_title_font_weight": {
      "control": {
          "field": "SelectControl",
          "label": "Font Weight",
          "props": {
              "options": [{
                      "label": "100",
                      "value": "100"
                  },
                  {
                      "label": "200",
                      "value": "200"
                  },
                  {
                      "label": "300",
                      "value": "300"
                  },
                  {
                      "label": "400",
                      "value": "400"
                  },
                  {
                      "label": "500",
                      "value": "500"
                  },
                  {
                      "label": "600",
                      "value": "600"
                  },
                  {
                      "label": "700",
                      "value": "700"
                  },
                  {
                      "label": "800",
                      "value": "800"
                  },
                  {
                      "label": "900",
                      "value": "900"
                  }
              ]
          }
      },
      "responsive": false,
      "type": "object",
      "default": {
          "desktop": 600,
          "tablet": 600,
          "mobile": 600
      }
  },
  "shopengine_heading_title_font_style": {
      "control": {
          "field": "SelectControl",
          "label": "Style",
          "props": {
              "options": [{
                      "label": "Default",
                      "value": "normal"
                  },
                  {
                      "label": "Normal",
                      "value": "normal"
                  },
                  {
                      "label": "Italic",
                      "value": "italic"
                  },
                  {
                      "label": "Oblique",
                      "value": "oblique"
                  }
              ]
          }
      },
      "responsive": false,
      "type": "object",
      "default": {
          "desktop": "normal",
          "tablet": "normal",
          "mobile": "normal"
      }
  },
  "shopengine_heading_title_text_transform": {
      "control": {
          "field": "SelectControl",
          "label": "Transform",
          "props": {
              "options": [{
                      "label": "Default",
                      "value": "initial"
                  },
                  {
                      "label": "Uppercase",
                      "value": "uppercase"
                  },
                  {
                      "label": "Lowercase",
                      "value": "lowercase"
                  },
                  {
                      "label": "Capitalize",
                      "value": "capitalize"
                  }
              ]
          }
      },
      "responsive": false,
      "type": "object",
      "default": {
          "desktop": "initial",
          "tablet": "initial",
          "mobile": "initial"
      }
  },
  "shopengine_heading_title_line_height": {
      "control": {
          "field": "RangeControl",
          "label": "Line Height(px)",
          "props": {
             "allowReset": true,
              "min": 0,
              "max": 100,
              "step": 1
          }
      },
      "responsive": true,
      "type": "object",
      "default": {
          "desktop": 24
      }
  },
  "shopengine_heading_title_letter_spacing": {
      "control": {
          "field": "RangeControl",
          "label": "Letter Spacing(px)",
          "props": {
              "allowReset": true,
              "min": 0,
              "max": 100,
              "step": 1
          }
      },
      "responsive": true,
      "type": "object",
      "default": {
          "desktop": 0.1
      }
  },
  "shopengine_heading_title_wordspace": {
      "control": {
          "field": "RangeControl",
          "label": "Word Spacing(px)",
          "props": {
              "allowReset": true,
              "min": 0,
              "max": 100,
              "step": 1
          }
      },
      "responsive": true,
      "type": "object",
      "default": {
          "desktop": ""
      }
  },
  "shopengine_heading_title_typography_end": {
      "type": "PopoverControl"
  },

   "shopengine_heading_style_panel_end":{
      "type":"PanelBody"
      
   }
}